{
  "name": "starter",
  "version": "0.0.0",
  "description": "...",
  "keywords": [],
  "homepage": "https://github.com/jbenner-radham/node-changelog#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-changelog/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-changelog.git"
  },
  "license": "BSD-3-Clause",
  "author": "James Benner <hello@jamesbenner.com> (https://www.jamesbenner.com/)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "bin": {
    "changelog": "./dist/esm/cli.js"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "bin": "tsx ./src/cli.ts",
    "prebuild": "rimraf ./dist",
    "build": "duo-build --packages external --platform node",
    "postbuild": "is-x ./dist/esm/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "start": "tsx ./src/index.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "@radham/semver": "^1.0.0",
    "hosted-git-info": "^9.0.2",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm": "^3.1.0",
    "mdast-util-to-markdown": "^2.1.2",
    "meow": "^14.0.0",
    "meowtastic": "^0.6.0",
    "micromark-extension-gfm": "^3.0.0",
    "micromark-util-normalize-identifier": "^2.0.1",
    "unist-builder": "^4.0.0",
    "unist-util-filter": "^5.0.1",
    "unist-util-flatmap": "^1.0.0",
    "unist-util-remove-position": "^5.0.0",
    "unist-util-select": "^5.1.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/json": "^0.13.2",
    "@radham/eslint-config": "^8.1.0",
    "@radham/is-x": "^0.5.1",
    "@types/hosted-git-info": "^3.0.5",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.18.8",
    "@types/unist": "^3.0.3",
    "@vitest/coverage-v8": "^4.0.4",
    "duo-build": "^0.2.1",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "rimraf": "^6.0.1",
    "sort-package-json": "^3.4.0",
    "tsx": "^4.20.6",
    "type-fest": "^5.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22"
  }
}
